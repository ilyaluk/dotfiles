[
  {
    "context": "Editor",
    "bindings": {
      "cmd-1": ["workspace::ActivatePane", 0],
      "ctrl-alt-shift-down": "copilot::NextSuggestion",
      "ctrl-alt-shift-up": "copilot::PreviousSuggestion",
      "cmd-shift-o": "outline::Toggle",
      "cmd-shift-b": "editor::FindAllReferences",
      "cmd-alt-f7": "editor::GoToTypeDefinition",
      "ctrl-k ctrl-g": [
        "editor::SelectNext",
        {
          "replace_newest": true
        }
      ]
    }
  },
  {
    "context": "(((((Dock || Workspace) || OutlinePanel) || ProjectPanel) || CollabPanel) || (Editor && mode == auto_height))",
    "bindings": {
      "escape": "editor::ToggleFocus"
    }
  },
  {
    "context": "(Editor && edit_prediction_conflict)",
    "bindings": {
      "ctrl-alt-tab": "editor::AcceptEditPrediction"
    }
  },
  {
    "context": "(Editor && edit_prediction)",
    "bindings": {
      "ctrl-alt-tab": "editor::AcceptEditPrediction"
    }
  }
]
