[
  {
    "context": "Editor",
    "bindings": {
      "cmd-1": ["workspace::ActivatePane", 0],
      "ctrl-alt-shift-down": "copilot::NextSuggestion",
      "ctrl-alt-shift-up": "copilot::PreviousSuggestion",
      "cmd-shift-o": "outline::Toggle",
      "cmd-shift-b": "editor::FindAllReferences",
      "cmd-alt-f7": "editor::GoToTypeDefinition",
      "ctrl-k ctrl-g": [
        "editor::SelectNext",
        {
          "replace_newest": true
        }
      ]
    }
  }
]
